!function(e){e(function(){e(".hasChildren").click(function(){var n=e(this);n.parent("li").parent().children("li").children(".sub-menu").not(n.next(".sub-menu")).slideUp("slow"),n.next(".sub-menu").slideToggle("slow"),n.toggleClass("open")}),e(".leader .button").click(function(){e(".off-canvas").addClass("is-open")}),e("#top-off-canvas .close-button, .off-canvas-overlay").click(function(){e(".off-canvas").removeClass("is-open")})})}(jQuery);