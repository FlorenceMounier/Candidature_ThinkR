var smart_settings=JSON.parse(document.querySelector("[data-selector='oas-settings-json']").innerHTML);console.log(smart_settings);var diffusionId,oas_setup_done=!1,oas_habillage=!1,habillage_width=1280,sas=sas||{};function OAS_setup(e,s){if(!oas_setup_done){oas_setup_done=!0;for(var t=[],i=[],a=e.split(","),o=0;o<=a.length;o++)void 0!==smart_settings.formatid[a[o]]&&t.push({id:smart_settings.formatid[a[o]]});var r=s.split("/"),u=[],n=[];i.push({sites:"all"}),i.push({domaine:r[0]});for(o=1;o<r.length-1;o++){var d=JSON.parse('{"r'+o+'": "'+r[o]+'"}');u.push(r[o]),n.push(r[o]),i.push(d)}n.push(r[r.length-1]),i.push({rubrique:u.join("/")}),i.push({rubrique_home:n.join("/")}),i.push({page:r[r.length-1]}),targets=[];for(o=0;o<=i.length;o++)for(keyword in i[o])targets.push(keyword+"="+i[o][keyword]);sas.cmd=sas.cmd||[],sas.cmd.push(function(){sas.setup({networkid:smart_settings.networkid,domain:smart_settings.url,async:!0})}),sas.cmd.push(function(){sas.call("onecall",{siteId:smart_settings.siteid,pageId:smart_settings.pageid,formats:t,target:targets.join(";")})})}}function OAS_AD(e){var s;void 0!==smart_settings.formatid[e]&&0!=oas_setup_done&&(s=smart_settings.formatid[e],document.write('<div id="sas_'+s+'"></div>'),sas.cmd.push(function(){sas.render(s)}))}var OAS_debounce=function(o,r,u){var n=void 0,d=void 0;return function(){for(var e=arguments.length,s=Array(e),t=0;t<e;t++)s[t]=arguments[t];var i=this,a=u&&!n;return clearTimeout(n),n=setTimeout(function(){n=null,u||(d=o.apply(i,s))},r),d=a?o.apply(i,s):d}},update_habillage_size=OAS_debounce(function(){var e=jQuery(window).width(),s=(jQuery(".leader"),200*e/habillage_width),t=calculate_top_height(),i=jQuery("#superpage"),a=jQuery("#sas-relDiv_"+diffusionId),o=jQuery("#sas-clickMap3_"+diffusionId),r=jQuery("#sas-clickMap1_"+diffusionId),u=jQuery("#sas-clickMap11_"+diffusionId),n=jQuery("#sas-clickMap2_"+diffusionId),d=jQuery("#sas-clickMap22_"+diffusionId);superpage_height=200,jQuery("#study-menu").hasClass("fixedTop")&&(superpage_height=236),a.css({position:"unset"}),o.css({left:0,position:"absolute","z-index":100}),r.css({left:0,position:"absolute","z-index":100}),n.css({left:"unset",right:0,position:"absolute","z-index":100}),1024<e?(document.body.style.backgroundSize="inherit",i.css({"margin-top":superpage_height+"px"}),diffusionId&&(o.css({top:t+"px",height:"200px",width:e+"px"}),r.css({top:t}),u.css({width:i.offset().left+"px",height:i.height()+t+"px"}),n.css({top:t}),d.css({width:jQuery(window).width()-(i.offset().left+i.outerWidth())+"px",height:i.height()+t+"px"}),u.show(),d.show())):(jQuery(".leader").css("position","fixed"),document.body.style.backgroundSize="100%",i.css({"margin-top":s+"px"}),diffusionId&&(o.css({top:t+"px",height:s+"px",width:e+"px"}),u.hide(),d.hide())),document.body.style.backgroundPosition="50% "+t+"px"},300);function OAS_habillage(e){diffusionId=e;calculate_top_height();var s=jQuery("#superpage"),e=jQuery("#homepage");oas_habillage=!0,document.body.style.backgroundRepeat="no-repeat",s.addClass("row"),e.css({height:"auto"}),jQuery(window).resize(update_habillage_size),jQuery(window).scroll(update_habillage_size),jQuery(function(){jQuery(window).trigger("resize")})}function calculate_top_height(){var e=jQuery(".rub-nav-wrapper"),s=+jQuery(".leader").height();return jQuery("#study-menu").hasClass("fixedTop")&&1024<jQuery(window).width()&&(s+=jQuery("#study-menu").outerHeight()),0<e.length&&(s+=e.outerHeight()),Math.ceil(s)}